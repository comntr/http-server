{
  "scripts": {
    "start": "npm i; npm run build; npm run http",
    "clean": "rm -rf ./bin",
    "build": "npm run clean; node_modules/.bin/tsc",
    "http": "sudo node bin/http -p 42751 -r /var/lib/comntr -z 1024",
    "test-perf": "npm run build; rm -r /tmp/comntr; node test/perf; du -sh /tmp/comntr",
    "test": "npm run build; node test/func",
    "scp-push": "scp -r src *.json root@comntr.live:~/comntr.io"
  },
  "dependencies": {
    "@types/lru-cache": "^5.1.0",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^12.0.7",
    "supercop.wasm": "5.0.1",
    "commander": "2.19.0",
    "lru-cache": "5.1.1",
    "mkdirp": "0.5.1",
    "sha1": "1.1.1",
    "typescript": "3.5.1"
  }
}
